<p id="notice"><%= notice %></p>

<p>
  <strong>parent:</strong>
  <%= @tag.parent_name %>
</p>

<p>
  <strong>name:</strong>
  <%= @tag.name %>
</p>

<p>
  <strong>Math Description:</strong>
  <% if @tag.normalized_alias_text  %>
  <%= @tag.normalized_alias_text.html_safe %>
  <% else %>
  <%= @tag.alias.html_safe %>
  <% end %>
</p>

<p>
  <strong>Level:</strong>
  <%= @tag.level %>
</p>


<%= link_to 'Edit', edit_tag_path(@tag) %> |
<%= link_to 'Back', tags_path %>


<script language="javascript" type="text/javascript">setTimeout("location.reload();",20000);</script>
